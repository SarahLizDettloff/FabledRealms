[application]

config/name="FabledRealms"
config/description="​In this fast-paced, action-packed side-scrolling game, you play as a brave hero who must fight off waves of slimy blobs. You play as a character who wakes up with no memories of who they are or where they come from. You must navigate through a mysterious and dangerous world, encountering various items who may have clues to your past.​"
run/main_scene="res://Scenes/MainMenu.tscn"
config/icon="res://iconblob.png"

[autoload]

Global="*res://Scripts/global.gd"

[display]

window/size/width=320
window/size/height=224
window/stretch/mode="2d"
window/stretch/aspect="keep"

